/* Self Task*/

#include<stdio.h>

int main()

{
int a,b;

    printf("Enter First No:");
    scanf("%d",&a);
    printf("Enter Second No:");
    scanf("%d",&b);
    printf("Addition :%d\n",(a+b));
    printf("Substraction :%d\n",(a-b));
    printf("Mltiplication :%d\n",(a*b));
    printf("Division :%d\n",(a/b));
    printf("Modulus :%d\n",(a%b));

}